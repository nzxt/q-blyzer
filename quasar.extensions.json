{
  "@quasar/typescript": {
    "webpack": "plugin",
    "rename": true
  },
  "@quasar/dotenv": {
    "env_development": ".env.development",
    "env_production": ".env.production",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": true
  },
  "@quasar/icon-genie": {
    "minify_dev": "pngquant",
    "minify_build": "pngcrush",
    "cordova": {
      "background_color": "#ffffff",
      "splashscreen_type": "pure"
    },
    "build_always": false,
    "__internal": {
      "dev": {},
      "build": {}
    }
  }
}