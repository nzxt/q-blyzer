{
  "@quasar/typescript": {
    "webpack": "plugin",
    "rename": true
  },
  "@quasar/dotenv": {
    "env_development": ".env.development",
    "env_production": ".env.production",
    "common_root_object": "none",
    "create_env_files": true,
    "add_env_to_gitignore": true
  },
  "@quasar/icon-genie": {
    "minify_dev": "pngquant",
    "minify_build": "optipng",
    "cordova": {
      "background_color": "#ffffff",
      "splashscreen_type": "pure"
    },
    "build_always": false,
    "__internal": {
      "dev": {
        "spa": {
          "iconHash": "31644f4d7bb5a3a1365c7c11c9dd1210"
        }
      },
      "build": {
        "pwa": {
          "iconHash": "31644f4d7bb5a3a1365c7c11c9dd1210"
        },
        "cordova": {
          "iconHash": "31644f4d7bb5a3a1365c7c11c9dd1210",
          "splashscreenHash": "79264ebd4f93268b461c3dae5add0e5a",
          "splashscreenType": "pure",
          "backgroundColor": "#ffffff"
        }
      }
    }
  },
  "auth-token-based": {
    "register_route": "/account/registration",
    "verification_route": false,
    "login_route": "/account/login",
    "password_forgot_route": false,
    "password_reset_route": false,
    "fetch_user_route": "/account/getprofile",
    "superuser_functionality": true
  }
}